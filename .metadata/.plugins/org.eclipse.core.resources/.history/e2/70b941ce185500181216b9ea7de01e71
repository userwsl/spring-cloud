package com.parent.base;

import java.util.Scanner;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args ){
    	//声明scanner对象
    	Scanner scanner = new Scanner(System.in);      
    	while(scanner.hasNext()) {
    		System.out.println(scanner.nextInt());
    	}
    }
}
