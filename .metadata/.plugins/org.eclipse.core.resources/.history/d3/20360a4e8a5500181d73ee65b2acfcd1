package com.parent.base;

/**
 * @description 是个类，类的话，只要定义一下基本的一些属性，一些行为
 * @author slwu
 *
 */
public class Person {

	//身高
	private int height;
	//性别
	private String sex;
	//年龄
	private int age;
	
	public Person() {};
	
	public Person(int height, String sex, int age) {
		this.height = height;
		this.sex = sex;
		this.age = age;
	}
	
}
